    <!-- Intro Header -->
    <header class="intro">
      <div class="intro-body">
        <div class="container">
          <div class="row">
            <div class="col-md-8 col-md-offset-2">
              <%# <div><img src = "img/nodeschool.svg" style = "width: 300px"/></div>%>
              <h1 class="brand-heading">
                Nodeschool Pereira<br>
                <a href="http://jointdeveloper.com" target="_blank" class="jointDeveloper">jointDeveloper</a>
              </h1>
              <p class="intro-text">Aprende a programar con Node.js.</p>
              <p class="intro-text">
                Sábado 5 de Noviembre del 2016, 8:00AM-5:00PM<br>
                Universidad Tecnológica de Pereira</p>
              <a href="#about" class="btn btn-circle page-scroll">
                <i class="fa fa-angle-double-down animated"></i>
              </a>
            </div>
          </div>
        </div>
      </div>
